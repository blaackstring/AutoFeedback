{
    "manifest_version": 3,
    "name": "Select 5th Option",
    "version": "1.1",
    "description": "Automatically check the input in the 5th option of all containers.",
    "permissions": ["scripting", "activeTab"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_title": "Run Script"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  